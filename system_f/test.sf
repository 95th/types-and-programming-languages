let add = fix \f: Nat->Nat->Nat. \m: Nat. \n: Nat.
	case (m, n) of 
		| (0, n) => n,
		| x => succ (f (pred m) n)
in let mul = fix \f: Nat->Nat->Nat. \m: Nat. \n: Nat.
	case (m, n) of 
		| (0, n) => n,
		| x => add n (f (pred m) n)
in let fact = fix \f: Nat->Nat. \m: Nat. 
	case m of 
		| 0 => 1 
		| _ => mul m (f (pred m))
